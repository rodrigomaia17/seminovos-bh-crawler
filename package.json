  "scripts": {
    "dev": "concurrently --kill-others \"npm run build-watch\" \"babel-node app.js\"",
    "lint": "eslint api app crawler models --ext .js,.jsx",
    "lint-fix": "eslint --fix api app crawler models --ext .js,.jsx",
